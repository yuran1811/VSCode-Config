{
	"debug.onTaskErrors": "abort",

	"window.autoDetectColorScheme": true,

	"editor.bracketPairColorization.enabled": true,
	// "editor.codeActionsOnSave": { "source.organizeImports": true },
	"editor.codeLensFontFamily": "Fira Code",
	"editor.cursorBlinking": "solid",
	"editor.cursorWidth": 2,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.definitionLinkOpensInPeek": true,
	"editor.fontFamily": "'Fira Code'",
	"editor.fontLigatures": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": false,
	"editor.insertSpaces": false,
	"editor.lineHeight": 1.7,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.renderWhitespace": "none",
	"editor.suggest.preview": true,
	"editor.suggestSelection": "first",
	"editor.wordWrap": "on",

	"javascript.autoClosingTags": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.suggest.autoImports": true,
	"javascript.updateImportsOnFileMove.enabled": "always",

	"typescript.autoClosingTags": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.suggest.autoImports": true,
	"typescript.updateImportsOnFileMove.enabled": "always",

	"security.workspace.trust.untrustedFiles": "open",

	"prettier.endOfLine": "auto",
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,

	"extensions.autoUpdate": "onlyEnabledExtensions",
	"extensions.ignoreRecommendations": true,

	"git.confirmSync": false,

	"material-icon-theme.hidesExplorerArrows": true,

	"liveServer.settings.CustomBrowser": "chrome",
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.donotVerifyTags": true,
	// "liveServer.settings.fullReload": true,
	// "liveSassCompile.settings.autoprefix": null,
	"liveSassCompile.settings.showOutputWindow": false,
	"liveSassCompile.settings.generateMap": false,
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".css",
			"savePath": "~../css"
		}
	],

	"terminal.integrated.defaultProfile.windows": "Bash",
	"terminal.integrated.profiles.windows": {
		"cmd": {
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			],
			"args": [],
			"icon": "terminal-cmd"
		},
		"Bash": { "path": "D:\\Program\\Git\\bin\\bash.exe" }
	},
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.enableMultiLinePasteWarning": false,

	"workbench.colorTheme": "Evondev Dracula Darker Contrast",
	"workbench.editor.enablePreview": false,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "none",

	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.autoSave": "afterDelay",
	"files.associations": {
		"*.ejs": "html"
	},

	"html-to-css-autocompletion.autocompletionFilesScope": "linked-files",
	"html-to-css-autocompletion.getSelectorsFromFileTypes": [
		"html",
		"php",
		"ejs"
	],
	"html.autoClosingTags": true,
	"html.format.indentInnerHtml": true,

	"vsintellicode.features.apiExamples": "enabled",
	"vsintellicode.features.python.deepLearning": "enabled",
	"vsintellicode.java.completionsEnabled": false,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"vsintellicode.python.completionsEnabled": false,

	"vscode_custom_css.imports": ["file:///D:/Program/vscode/style.css"],

	// More theme: https://glitchbone.github.io/vscode-base16-term/#/tomorrow-night
	"workbench.colorCustomizations": {
		"terminal.ansiBlack": "#282C34",
		"terminal.ansiBlue": "#61AFEF",
		"terminal.ansiBrightBlack": "#545862",
		"terminal.ansiBrightBlue": "#61AFEF",
		"terminal.ansiBrightCyan": "#56B6C2",
		"terminal.ansiBrightGreen": "#98C379",
		"terminal.ansiBrightMagenta": "#C678DD",
		"terminal.ansiBrightRed": "#E06C75",
		"terminal.ansiBrightWhite": "#C8CCD4",
		"terminal.ansiBrightYellow": "#E5C07B",
		"terminal.ansiCyan": "#56B6C2",
		"terminal.ansiGreen": "#98C379",
		"terminal.ansiMagenta": "#C678DD",
		"terminal.ansiRed": "#E06C75",
		"terminal.ansiWhite": "#ABB2BF",
		"terminal.ansiYellow": "#E5C07B",
		"terminal.foreground": "#ABB2BF",
		"terminalCursor.background": "#ABB2BF",
		"terminalCursor.foreground": "#ABB2BF"
	}
}
